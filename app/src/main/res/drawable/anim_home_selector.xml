<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/state_selected" android:drawable="@drawable/ic_home_s" android:state_selected="true" />

    <item android:id="@+id/state_default" android:drawable="@drawable/ic_home" />

    <transition android:drawable="@drawable/avd_home_d_to_s" android:fromId="@id/state_default" android:toId="@id/state_selected" />

    <transition android:drawable="@drawable/avd_home_s_to_d" android:fromId="@id/state_selected" android:toId="@id/state_default" />

</selector>